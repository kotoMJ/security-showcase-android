<manifest xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
	package="cz.kotox.keystorecompat19">

	<uses-sdk tools:overrideLibrary="cz.kotox.base" />

	<application
		android:allowBackup="true"
		android:supportsRtl="true">

		<receiver
			android:name="cz.kotox.keystorecompat19.SecurityDeviceAdmin"
			android:label="${kitkatDeviceAdminLabel}"
			android:description="@string/kc_auth_desc"
			android:permission="android.permission.BIND_DEVICE_ADMIN">
			<meta-data
				android:name="android.app.device_admin"
				android:resource="@xml/device_admin" />
			<intent-filter>
				<action android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
				<action android:name="android.app.action.DEVICE_ADMIN_DISABLED" />
			</intent-filter>
		</receiver>

	</application>

</manifest>